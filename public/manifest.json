{
    "name": "McDash",
    "description": "A customisable New Tab Dashboard",
    "version": "0.1",
    "manifest_version": 2,
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "options_page": "index.html#/options",
    "permissions": [
        "storage",
        "tabs",
        "https://api.darksky.net/"
    ]
}