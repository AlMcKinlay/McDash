{
  "name": "McDash",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "concurrently \"yarn dev:vue\" \"node server\\app.js\"",
    "dev:vue": "vue-cli-service serve",
    "prod": "node server\\app.js --prod"
  },
  "dependencies": {
    "archiver": "^3.0.0",
    "concurrently": "^4.1.0",
    "express-http-proxy": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.3",
    "register-service-worker": "^1.5.2",
    "request-promise": "^4.2.2",
    "sass-loader": "^7.1.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.2",
    "vue-weathericons": "^0.0.3",
    "weathericons": "^2.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
