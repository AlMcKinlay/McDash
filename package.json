{
  "name": "McDash",
  "version": "0.2.1",
  "scripts": {
    "build": "npm run build:vue && npm run build:extension",
    "build:vue": "vue-cli-service build",
    "build:extension": "node build.js",
    "lint": "eslint --ext .js,.vue src",
    "dev": "vue-cli-service serve"
  },
  "dependencies": {
    "archiver": "5.3.0",
    "babel-eslint": "10.1.0",
    "concurrently": "7.0.0",
    "eslint": "8.10.0",
    "eslint-plugin-vue": "8.5.0",
    "express-http-proxy": "1.6.3",
    "html-webpack-plugin": "5.5.0",
    "node-sass": "6.0.1",
    "request-promise": "4.2.6",
    "sass-loader": "10.2.1",
    "vue": "3.2.31",
    "vue-router": "4.0.12",
    "vue-weathericons": "0.0.3",
    "vuex": "4.0.2",
    "vuex-persistedstate": "4.1.0",
    "weathericons": "2.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "5.0.1",
    "@vue/cli-service": "5.0.1",
    "vue-template-compiler": "2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "release-it": {
    "git": {
      "commit": false,
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true,
      "releaseName": "${version}",
      "assets": "./dist/dash.zip"
    },
    "npm": {
      "publish": false
    }
  }
}
