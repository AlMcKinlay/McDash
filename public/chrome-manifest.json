{
    "name": "McDash",
    "description": "A customisable New Tab Dashboard",
    "version": "0.1",
    "manifest_version": 2,
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "options_page": "extension/settings.html",
    "optional_permissions": [ "tabs", "http://*/", "https://*/" ],
    "permissions": [
        "storage"
    ]
}